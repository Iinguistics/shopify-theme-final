  <style>
   footer{
       background: {{ section.settings.color_footer_bg }};
       color: {{ section.settings.color_footer_text }};
   }
footer .footer-menu .menu-title,
footer .social-title {
	color: {{ section.settings.color_footer_menu_title }};
}
footer .footer-menu a {
	color: {{ section.settings.color_footer_menu_title }};
}
footer .address {
	color: {{ section.settings.color_footer_text }};
}


  </style>
  <footer>
    <div class="container">
      <div class="row">
        <div class="col-md-6">
          <div class="row">
          {% for block in section.blocks %}
            {%- assign footer_linklist = block.settings.menu -%}
            <div class="col-md-4">
              <div class="footer-menu footer-fade-in">
                <span class="menu-title">{{ block.settings.title }}</span>
                <ul>
                {%- for link in linklists[footer_linklist].links -%}
                  <li>
                    <a href="{{ link.url }}">{{ link.title | escape }}</a>
                  </li>
                  {% endfor %}
                </ul>
              </div>
            </div>
            {% endfor %}
          </div>
        </div>
        <div class="col-md-6">
          <div class="footer-fade-in">
            <div class="social-title">Follow Us</div>
            <ul class="social-icons">
            {% if settings.social_facebook_link != blank %}
              <li>
                <a href={{ settings.social_facebook_link }}>
                  <i class="fa fa-facebook" aria-hidden="true"></i>
                </a>
              </li>
              {% endif %}
            {% if settings.social_twitter_link != blank %}
              <li>
                <a href={{ settings.social_facebook_link }}>
                  <i class="fa fa-twitter" aria-hidden="true"></i>
                </a>
              </li>
              {% endif %}
              {% if settings.social_pinterest_link != blank %}
              <li>
                <a href={{ settings.social_pinterest_link }}>
                  <i class="fa fa-pinterest" aria-hidden="true"></i>
                </a>
              </li>
              {% endif %}
            </ul>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-sm-12 footer-fade-in">
          <hr>
          <span class="address">Storename 23 Grand Ave, Los Angeles, California 11234</span>
        </div>
      </div>
    </div>
  </footer>


